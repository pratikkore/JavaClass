package Test2;

public class Problem8 {

//    1 2 3 4 5
//      1 2 3 4
//        1 2 3
//          1 2
//            1

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		for (int i = 1; i <= 5; i++) {
			for (int j = 5; j <= i; j--) {
				System.out.print(i);
			}
			System.out.println();
		}
	}

}
