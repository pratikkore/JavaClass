package Day8Assignment;

class Bird {
	void test() {
		System.out.println("Every bird can Fly high");
	}
}

class pecock extends Bird {
	public void test() {
		System.out.println("Pecock is not Flying High ..");
	}
}

public class Overriding {

	public static void main(String[] args) {

		pecock a=new pecock();
		a.test();
	}

}
