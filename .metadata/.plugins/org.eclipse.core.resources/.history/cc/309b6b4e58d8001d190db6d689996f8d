package Day11_4_23;

public class Array {

	public void display(char arr[]) {
		for (char i = 0; i < arr.length; i++) {
			System.out.println(arr[i] + 2);
		}
	}

	public static void main(String[] args) {

		char arr[] = { 'a', 'e', 's', 'z' };

		Array obj = new Array();
		obj.display(arr);

	}

}
