package Day6Assignment;

public class StudentInfo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Student obj = new Student();
		obj.setId(123);
		obj.setName("Pratik");
		obj.setPercentage(87.21);
		obj.setAddress("Pune");

		System.out.println("Student Id= " + obj.getId() +"Student Name"+ obj.getName() +"Student Address : "+ obj.getAddress()+"Student Percentage" + obj.getPercentage());
	}

}
