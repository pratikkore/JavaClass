package Test2;

public class Problem8 {

//    1 2 3 4 5
//      1 2 3 4
//        1 2 3
//          1 2
//            1

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		for (int i = 1; i <= 5; i++) {

			for (int k = 1; k <= i; k++) {
				System.out.print(" ");
			}
			for (int j = 1; j >= 5; j++) {
				System.out.print(j);
			}
			System.out.println();
		}
	}

}
